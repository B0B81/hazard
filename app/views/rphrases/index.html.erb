<p id="notice"><%= notice %></p>

<h1>Rphrases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rphrases.each do |rphrase| %>
      <tr>
        <td><%= link_to 'Show', rphrase %></td>
        <td><%= link_to 'Edit', edit_rphrase_path(rphrase) %></td>
        <td><%= link_to 'Destroy', rphrase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rphrase', new_rphrase_path %>
